<?xml version="1.0" standalone="no"?>
<!DOCTYPE DAVEfunc PUBLIC "-//AIAA//DTD for Flight Dynamic Models - Functions 2.0//EN"  
          "http://www.daveml.org/DTDs/2p0/DAVEfunc.dtd">
<DAVEfunc xmlns="http://daveml.org/2010/DAVEML">

  <fileHeader name="GTM Aerodynamic Model">
    <author name="Trey Arthur" org="NASA Langley Research Center" email="trey.arthur@nasa.gov">
      <address>MS 152 NASA, Hampton, VA 23681</address>
    </author>

    <creationDate date="2025-04-18"/>
    <fileVersion>Version: 001</fileVersion>

    <description>
      An implementation of the GTM model as described in the paper:  A Generic Nonlinear Aerodynamic Model for Aircraft, Grauer and Morelli, 2014
    </description>
  </fileHeader>

  <!-- ================== -->
  <!--  Input variables   -->
  <!-- ================== -->

  <variableDef name="angleOfAttack" varID="alpha_deg" units="deg" symbol="&#x3B1;">
    <description> Instantaneous true angle-of-attack, in degrees </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="angleOfSideslip" varID="beta_deg" units="deg" symbol="&#x3B2;"
               sign="wind in right ear">
    <description> Instantaneous true angle-of-sideslip, in degrees </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="rollBodyRate" varID="p_deg_s" units="deg_s" sign="right wing down"
               symbol="p">
    <description> Body roll rate, rad/sec </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="pitchBodyRate" varID="q_deg_s" units="deg_s" sign="aircraft nose up"
               symbol="q">
    <description> Body pitch rate, rad/sec </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="yawBodyRate" varID="r_deg_s" units="deg_s" sign="aircraft nose right"
               symbol="r">
    <description> Body yaw rate, rad/sec </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="elevatorDeflection" varID="el_deg" units="deg" sign="trailing edge down"
               symbol="&#x3B4;e">
    <description> Elevator deflection angle, in degrees (+TED) </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="aileronDeflection" varID="ail_deg" units="deg" sign="left roll" symbol="&#x3B4;a">
    <description> Aileron deflection angle, in degrees (+LWD) </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="rudderDeflection" varID="rdr_deg" units="deg" sign="TEL" symbol="&#x3B4;r">
    <description> Rudder deflection angle, in degrees (+TEL) </description>
    <isStdAIAA/>
  </variableDef>

  <!-- ================== -->
  <!--      Constants     -->
  <!-- ================== -->

  <variableDef name="dtr" varID="dtr" units="rad_deg" initialValue="0.01745329">
    <description> Conversion constant from radians to degrees </description>
  </variableDef>

  <variableDef name="totalMass" varID="XMASS" units="slug" initialValue="1.55404754">
    <description>
      Total mass of GTM (50 lbm)
    </description>
    <isStdAIAA/>
  </variableDef>
  
  <variableDef name="bodyMomentOfInertia_X" varID="IXX" units="slugft2" initialValue="1.327">
    <description>
      Rolling moment of inertia about the body X axis
    </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="bodyMomentOfInertia_Y" varID="IYY" units="slugft2" initialValue="4.254">
    <description>
      Pitching moment of inertia about the body Y axis
    </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="bodyMomentOfInertia_Z" varID="IZZ" units="slugft2" initialValue="5.454">
    <description>
      Yawing moment of inertia about the body Z axis
    </description>
    <isStdAIAA/>
  </variableDef>
  
  <variableDef name="bodyProductOfInertia_XZ" varID="IXZ" units="slugft2" initialValue="0.120">
    <description>
      XZ moment of inertia about the body Z axis
    </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="referenceWingChord" varID="cbar" units="ft" initialValue="0.915">
    <description> Length of aerodynamic chord, ft </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="referenceWingSpan" varID="bspan" units="ft" initialValue="6.849">
    <description> Length of aerodynamic span, ft </description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="referenceWingArea" varID="SWING" units="ft2" initialValue="5.902">
    <description>Reference area of cannonball (assumed radius is 3")</description>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="XBodyPositionOfMRC" varID="xcgr" units="nd" initialValue="0.25">
    <description> 
      Default location of center of gravity relative to wing leading
      edge, expressed as a fraction of aerodynamic chord length.
    </description>
    <isStdAIAA/>
  </variableDef>

  <!-- ============================ -->
  <!--  Theta parameters variables  -->
  <!-- ============================ -->

