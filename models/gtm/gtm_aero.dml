  <variableDef name="k2" varID="k2" units="nd" initialValue="-0.078"></variableDef>
  <variableDef name="k3" varID="k3" units="nd" initialValue="-27.42"></variableDef>
  <variableDef name="k4" varID="k4" units="nd" initialValue="0.293"></variableDef>
  <variableDef name="k5" varID="k5" units="nd" initialValue="3.420"></variableDef>
  <variableDef name="k6" varID="k6" units="nd" initialValue="288.2"></variableDef>
  <variableDef name="k7" varID="k7" units="nd" initialValue="-0.040"></variableDef>
  <variableDef name="k8" varID="k8" units="nd" initialValue="1.819"></variableDef>
  <variableDef name="k9" varID="k9" units="nd" initialValue="-355.3"></variableDef>
  <variableDef name="k10" varID="k10" units="nd" initialValue="-6.563"></variableDef>
  
  <variableDef name="k11" varID="k11" units="nd" initialValue="-1.003"></variableDef>
  <variableDef name="k12" varID="k12" units="nd" initialValue="0.033"></variableDef>
  <variableDef name="k13" varID="k13" units="nd" initialValue="0.952"></variableDef>
  <variableDef name="k14" varID="k14" units="nd" initialValue="-0.009"></variableDef>
  <variableDef name="k15" varID="k15" units="nd" initialValue="0.253"></variableDef>
  
  <variableDef name="k16" varID="k16" units="nd" initialValue="0.016"></variableDef>
  <variableDef name="k17" varID="k17" units="nd" initialValue="5.343"></variableDef>
  <variableDef name="k18" varID="k18" units="nd" initialValue="30.78"></variableDef>
  <variableDef name="k19" varID="k19" units="nd" initialValue="0.396"></variableDef>
  <variableDef name="k20" varID="k20" units="nd" initialValue="12.03"></variableDef>
  <variableDef name="k21" varID="k21" units="nd" initialValue="0.506"></variableDef>
  <variableDef name="k22" varID="k22" units="nd" initialValue="-36.30"></variableDef>
  <variableDef name="k23" varID="k23" units="nd" initialValue="46.13"></variableDef>

  <variableDef name="k24" varID="k24" units="nd" initialValue="-0.109"></variableDef>
  <variableDef name="k25" varID="k25" units="nd" initialValue="-0.366"></variableDef>
  <variableDef name="k26" varID="k26" units="nd" initialValue="0.061"></variableDef>
  <variableDef name="k27" varID="k27" units="nd" initialValue="-0.079"></variableDef>
  <variableDef name="k28" varID="k28" units="nd" initialValue="0.021"></variableDef>
  
  <variableDef name="k29" varID="k29" units="nd" initialValue="0.182"></variableDef>
  <variableDef name="k30" varID="k30" units="nd" initialValue="-1.782"></variableDef>
  <variableDef name="k31" varID="k31" units="nd" initialValue="-44.34"></variableDef>
  <variableDef name="k32" varID="k32" units="nd" initialValue="-1.785"></variableDef>
  <variableDef name="k33" varID="k33" units="nd" initialValue="374.0"></variableDef>
  <variableDef name="k34" varID="k34" units="nd" initialValue="-1748."></variableDef>
  <variableDef name="k35" varID="k35" units="nd" initialValue="2.439"></variableDef>
  <variableDef name="k36" varID="k36" units="nd" initialValue="1949."></variableDef>
  <variableDef name="k37" varID="k37" units="nd" initialValue="-0.038"></variableDef>
  <variableDef name="k38" varID="k38" units="nd" initialValue="0.803"></variableDef>
  
  <variableDef name="k39" varID="k39" units="nd" initialValue="0.183"></variableDef>
  <variableDef name="k40" varID="k40" units="nd" initialValue="-0.022"></variableDef>
  <variableDef name="k41" varID="k41" units="nd" initialValue="-0.405"></variableDef>
  <variableDef name="k42" varID="k42" units="nd" initialValue="-0.009"></variableDef>
  <variableDef name="k43" varID="k43" units="nd" initialValue="-0.129"></variableDef>
  <variableDef name="k44" varID="k44" units="nd" initialValue="0.184"></variableDef>
  <variableDef name="k45" varID="k45" units="nd" initialValue="-0.377"></variableDef>

  <!-- ==================== -->
  <!--  Internal variables  -->
  <!-- ==================== -->

  <variableDef name="alpha_rad" varID="alpha" units="rad">
      <description>
        Convert AoA from degrees to radians
      </description>
      <calculation>
        <python>{alpha_deg}*{dtr}</python>
      </calculation>
  </variableDef>
  <variableDef name="alpha2_rad" varID="alpha2" units="rad">
      <description>
        AoA squared
      </description>
      <calculation>
        <python>{alpha}*{alpha}</python>
      </calculation>
  </variableDef>
  <variableDef name="alpha3_rad" varID="alpha3" units="rad">
      <description>
        AoA cubed
      </description>
      <calculation>
        <python>{alpha}*{alpha2}</python>
      </calculation>
  </variableDef>
  <variableDef name="alpha4_rad" varID="alpha4" units="rad">
      <description>
        AoA to the power of 4
      </description>
      <calculation>
        <python>{alpha}*{alpha3}</python>
      </calculation>
  </variableDef>

  <variableDef name="beta_rad" varID="beta" units="rad">
      <description>
        Convert sideslip from degrees to radians
      </description>
      <calculation>
        <python>{beta_deg}*{dtr}</python>
      </calculation>
  </variableDef>

  <variableDef name="beta2_rad" varID="beta2" units="rad2">
      <description>
        sideslip squared
      </description>
      <calculation>
        <python>{beta}*{beta}</python>
      </calculation>
  </variableDef>

  <variableDef name="beta3_rad" varID="beta3" units="rad3">
      <description>
        sideslip cubed
      </description>
      <calculation>
        <python>{beta}*{beta2}</python>
      </calculation>
  </variableDef>

  <variableDef name="rollBodyRate_rad_s" varID="p" units="rad_s">
      <description>
        roll rate degrees/sec to radians/sec
      </description>
      <calculation>
        <python>{p_deg_s}*{dtr}</python>
      </calculation>
  </variableDef>

  <variableDef name="pitchBodyRate_rad_s" varID="q" units="rad_s">
      <description>
        pitch rate degrees/sec to radians/sec
      </description>
      <calculation>
        <python>{q_deg_s}*{dtr}</python>
      </calculation>
  </variableDef>

  <variableDef name="yawBodyRate_rad_s" varID="r" units="rad_s">
      <description>
        yaw rate degrees/sec to radians/sec
      </description>
      <calculation>
        <python>{r_deg_s}*{dtr}</python>
      </calculation>
  </variableDef>

  <variableDef name="el_rad" varID="el" units="rad">
      <description>
        elevator degrees to radians
      </description>
      <calculation>
        <python>{el_deg}*{dtr}</python>
      </calculation>
  </variableDef>

  <variableDef name="ail_rad" varID="ail" units="rad">
      <description>
        aileron degrees to radians
      </description>
      <calculation>
        <python>{ail_deg}*{dtr}</python>
      </calculation>
  </variableDef>

  <variableDef name="rdr_rad" varID="rdr" units="rad">
      <description>
        rudder degrees to radians
      </description>
      <calculation>
        <python>{rdr_deg}*{dtr}</python>
      </calculation>
  </variableDef>

  <!-- ================== -->
  <!--  Output variables  -->
  <!-- ================== -->

  <variableDef name="aeroBodyForceCoefficient_X" varID="cx" units="nd" sign="FWD">
    <description> 
      Total coefficient of force along the body X-axis.
    </description>
    <calculation>
      <python>
        {k1}+{k2}*{alpha}+{k3}*{alpha}*{q}+{k4}*{alpha}*{el}+{k5}*{alpha2}+\
        {k6}*{alpha2}*{q}+{k7}*{alpha2}*{el}+{k8}*{alpha3}+\
        {k9}*{alpha3}*{q}+{k10}*{alpha4}
      </python>
    </calculation>
    <isOutput/>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="aeroBodyForceCoefficient_Y" varID="cy" units="nd" sign="RIGHT">
    <description> 
      Total coefficient of force along the body Y-axis
    </description>
    <calculation>
      <python>{k11}*{beta}+{k12}*{p}+{k13}*{r}+{k14}*{ail}+{k15}*{rdr}</python>
    </calculation>
    <isOutput/>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="aeroBodyForceCoefficient_Z" varID="cz" units="nd" sign="DOWN">
    <description> 
      Total coefficient of force along the body Z-axis.
    </description>
    <calculation>
      <python>
        {k16}+{k17}*{alpha}+{k18}*{q}+{k19}*{el}+{k20}*{alpha}*{q}+\
        {k21}*{alpha2}+{k22}*{alpha3}+{k23}*{alpha4}
      </python>
    </calculation>
    <isOutput/>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="aeroBodyMomentCoefficient_Roll" varID="cl" units="nd" sign="RWD">
    <description> 
      Total coefficient of moment around the body X-axis (rolling
      moment)
    </description>
    <calculation>
      <python>{k24}*{beta}+{k25}*{p}+{k26}*{r}+{k27}*{ail}+{k28}*{rdr}</python>
    </calculation>
    <isOutput/>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="aeroBodyMomentCoefficient_Pitch" varID="cm" units="nd" sign="ANU">
    <description> 
      Total coefficient of moment around the body Y-axis (pitching
      moment)
    </description>
    <calculation>
      <python>
        {k29}+{k30}*{alpha}+{k31}*{q}+{k32}*{el}+{k33}*{alpha}*{q}+\
        {k34}*{alpha2}*{q}+{k35}*{alpha2}*{el}+{k36}*{alpha3}*{q}+\
        {k37}*{alpha3}*{el}+{k38}*{alpha4}
      </python>
    </calculation>
    <isOutput/>
    <isStdAIAA/>
  </variableDef>

  <variableDef name="aeroBodyMomentCoefficient_Yaw" varID="cn" units="nd" sign="ANR">
    <description> 
      Total coefficient of moment around the body Z-axis (yawing
      moment)
    </description>
    <calculation>
      <python>
        {k39}*{beta}+{k40}*{p}+{k41}*{r}+{k42}*{ail}+{k43}*{rdr}+\
        {k44}*{beta2}+{k45}*{beta3}
      </python>
    </calculation>
    <isOutput/>
    <isStdAIAA/>
  </variableDef>

